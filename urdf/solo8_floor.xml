                    <joint name="HL_KFE" pos="0 0 0" axis="0 1 0" limited="true" range="-40 -10" type="hinge"/>
                    <!-- <geom axisangle="0 1 0 -30" type="mesh" group="1" rgba="0.8 0.8 0.8 1" mesh="solo_lower_leg_left_side" /> -->
