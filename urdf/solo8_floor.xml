            </body>  <!-- End HR Leg Segment -->
        </body>  <!-- End Entire Body  -->
    </worldbody>
    <actuator>
        <motor name="FL_TOP" ctrllimited="true" ctrlrange="-1.0 1.0" joint="FL_HFE" gear="50 0 0 0 0 0"/>
        <motor name="FL_BOT" ctrllimited="true" ctrlrange="-1.0 1.0" joint="FL_KFE" gear="30 0 0 0 0 0"/>
        <motor name="FR_TOP" ctrllimited="true" ctrlrange="-1.0 1.0" joint="FR_HFE" gear="50 0 0 0 0 0"/>    
        <motor name="FR_BOT" ctrllimited="true" ctrlrange="-1.0 1.0" joint="FR_KFE" gear="30 0 0 0 0 0"/>
