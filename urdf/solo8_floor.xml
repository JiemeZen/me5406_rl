<mujoco model="solo">
    <compiler angle="degree" coordinate="local" inertiafromgeom="true"/>
    <option integrator="RK4" timestep="0.01"/>
    
    <default>
        <joint armature="1" damping="1" limited="true"/>
        <geom conaffinity="0" condim="3" density="5.0" friction="1 0.1 0.1" margin="0.01" rgba="0.8 0.6 0.4 1"/>
    </default>
    
    <asset>
        <mesh name="solo_body" file="solo_body.stl" />
        <mesh name="solo_upper_leg_left_side" file="solo_upper_leg_left_side.stl" />
        <mesh name="solo_lower_leg_left_side" file="solo_lower_leg_left_side.stl" />
        <mesh name="solo_foot" file="solo_foot.stl" />
        <mesh name="solo_upper_leg_right_side" file="solo_upper_leg_right_side.stl" />
        <mesh name="solo_lower_leg_right_side" file="solo_lower_leg_right_side.stl" />
        
        <texture builtin="gradient" height="100" rgb1="1 1 1" rgb2="0 0 0" type="skybox" width="100"/>
        <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
        <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
        <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
        <material name="geom" texture="texgeom" texuniform="true"/>
    </asset>
    <worldbody>
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
        <geom conaffinity="1" friction="1 .1 .1" condim="3" material="MatPlane" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="40 40 40" type="plane"/>
    
        <body name="solo_body" pos="0 0 0.26">
            <camera name="track" mode="trackcom" pos="0 -3 0.3" xyaxes="1 0 0 0 0 1"/>
            <geom type="mesh" group="1" rgba="0.8 0.8 0.8 1" mesh="solo_body" />
            <!-- <geom type="mesh" rgba="0.8 0.8 0.8 1" mesh="solo_body" /> -->
            <joint armature="0" axis="1 0 0" damping="0" limited="false" margin="0.01" name="root" pos="0 0 0" type="free"/>
            
            <body name="FL_UPPER_LEG" pos="0.19 0.1046 0">
                <inertial pos="1.377e-05 0.0193585 -0.078707" quat="0.705834 0.0424071 -0.0424071 0.705834" mass="0.148538" diaginertia="0.000417563 0.00041107 2.46069e-05" />
                <joint name="FL_HFE" pos="0 0 0" axis="0 1 0" limited="true" range="0 40" type="hinge"/>
                <geom axisangle="0 1 0 40" type="mesh" group="1" rgba="0.8 0.8 0.8 1" mesh="solo_upper_leg_left_side"/>
